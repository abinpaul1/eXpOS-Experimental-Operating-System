// Currently only implements Terminal Write

alias functionNum R1;
alias currentPID R2;
alias word R3;

if (functionNum!=3) then
  return;
endif;


//Acquiring Terminal
multipush(R0, R1, R2, R3);
//Storing function number in R1
R1 = 8;
R2 = currentPID;
call MOD_0;
multipop(R0, R1, R2, R3);
print word;


//Releasing terminal
multipush(R0, R1, R2, R3);
//Storing function number in R1
R1 = 9;
R2 = currentPID;
call MOD_0;
multipop(R0, R1, R2, R3);

return;
